# Ignore directories generated by Composer
/vendor
/web/core
/web/modules/contrib
/web/themes/contrib
/web/profiles/contrib
/web/libraries
/drush/contrib

# Ignore configuration files that may contain sensitive information
/web/sites/*/settings.php
/web/sites/*/services.yml
/web/sites/*/settings.local.php

# Ignore files generated by the local environment
/web/sites/*/files
/web/sites/*/private

# Ignore DDEV configuration files
/.ddev
.ddev/

# Ignore IDE specific files
/.idea
/.vscode
*.swp
*.swo
.DS_Store

# Ignore other temporary files
/tmp

# Database dumps (export scripts)
/data

# ngrok (local tunnel)
/ngrok-url.txt
/ngrok.log

# Environment files with credentials
.env
.env.*
!.env.example
